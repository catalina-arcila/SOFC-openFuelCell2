FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/solid";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 973.15;

boundaryField
{
    electrolyteSides
    {
        type            zeroGradient;
    }
    electrolyte_to_air
    {
        type            zeroGradient;
    }
    electrolyte_to_fuel
    {
        type            zeroGradient;
    }
}


