FoamFile
{
    version         2.0;
    format          ascii;
    class           volScalarField;
    object          phi;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 1.5; // uniform (0.566711 0 0);

boundaryField
{
    ".*"
    {
        type            zeroGradient;
    }
}


