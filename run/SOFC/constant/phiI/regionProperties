FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          cellProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

//- Add information here

control           false;
relax             -1.0e-10;

dissolveOnOff   true;
dissolved
{
    type        none;

    noneCoeffs
    {
        region      phiI;
        nd          0.12;
        rhoOnEW     1964;

        ksi
        (
            acl 1.3
            ccl 1.3
        );

        relax 5.0e-4;
    }
}

sigma
{
    ccl
    {
        sigmaModel          constantSigma;

        constantSigmaCoeffs
        {
            cellZone            ccl;
            sigma               15;
        }
    }

    acl
    {
        sigmaModel          constantSigma;

        constantSigmaCoeffs
        {
            cellZone            acl;
            sigma               15;
        }
    }

    membrane
    {
        sigmaModel          constantSigma;

        constantSigmaCoeffs
        {
            cellZone            membrane;
            sigma               15;
        }
    }
}
